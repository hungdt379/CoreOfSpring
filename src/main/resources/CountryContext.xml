<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="india" class="com.example.coreofspring.entity.Country">
        <constructor-arg index="0" value="India"/>
        <constructor-arg index="1" value="20000"/>
    </bean>
    <bean id="vietnam" class="com.example.coreofspring.entity.Country">
        <property name="name" value="Vietnam"/>
        <property name="population" value="10000"/>
    </bean>

</beans>